# AI Gateway Configuration (EJEMPLO - Copiar a .env)

# Puerto del servicio
PORT=5000

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# LLM Provider (gemini o openai)
LLM_PROVIDER=gemini

# URLs de servicios
GRAPHQL_SERVICE_URL=http://localhost:4000/graphql
REST_API_URL=https://salumedx-rest.onrender.com

# JWT Secret (debe coincidir con el Auth Service)
JWT_SECRET=your-secret-key-change-in-production-xyz123

# Configuraci√≥n del modelo
MODEL_TEMPERATURE=0.7
